{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "logAnalyticsWorkspaceName": {
            "type": "string",
            "metadata": {
                "description": "Required. Name of the Log Analytics workspace"
            }
        },
        "serviceTier": {
            "type": "string",
            "defaultValue": "PerNode",
            "allowedValues": [
                "Free",
                "Standalone",
                "PerNode",
                "PerGB2018"
            ],
            "metadata": {
                "description": "Required. Service Tier: Free, Standalone, PerGB or PerNode"
            }
        },
        "dataRetention": {
            "type": "int",
            "defaultValue": 365,
            "minValue": 0,
            "maxValue": 730,
            "metadata": {
                "description": "Required. Number of days data will be retained for"
            }
        },
        "location": {
            "type": "string",
            "allowedValues": [
                "Australia Central",
                "Australia East",
                "Australia Southeast",
                "Canada Central",
                "Central India",
                "Central US",
                "East Asia",
                "East US",
                "East US 2",
                "France Central",
                "Japan East",
                "Korea Central",
                "North Europe",
                "South Central US",
                "Southeast Asia",
                "UK South",
                "West Europe",
                "West US",
                "West US 2"
            ],
            "metadata": {
                "description": "Required. Region used when establishing the workspace"
            }
        },
        "diagnosticStorageAccountName": {
            "type": "string",
            "metadata": {
                "description": "Required. Diagnostic Storage Account name"
            }
        },
        "diagnosticStorageAccountId": {
            "type": "string",
            "metadata": {
                "description": "Required. Log Analytics workspace resource identifier"
            }
        },
        "diagnosticStorageAccountAccessKey": {
            "type": "securestring",
            "metadata": {
                "description": "Required. Diagnostic Storage Account key"
            }
        },
        "automationAccountId": {
            "type": "string",
            "defaultValue": "",
            "metadata": {
                "description": "Optional. Automation Account resource identifier, value used to create a LinkedService between Log Analytics and an Automation Account."
            }
        }
    },
    "variables": {
        "logAnalyticsSearchVersion": 1,
        "solutions": [
            {
                "name": "[concat('Updates', '(', parameters('logAnalyticsWorkspaceName'), ')')]",
                "galleryName": "Updates"
            },
            {
                "name": "[concat('AzureAutomation', '(', parameters('logAnalyticsWorkspaceName'), ')')]",
                "galleryName": "AzureAutomation"
            },
            {
                "name": "[concat('AntiMalware', '(', parameters('logAnalyticsWorkspaceName'), ')')]",
                "galleryName": "AntiMalware"
            },
            {
                "name": "[concat('SQLAssessment', '(', parameters('logAnalyticsWorkspaceName'), ')')]",
                "galleryName": "SQLAssessment"
            },
            {
                "name": "[concat('Security', '(', parameters('logAnalyticsWorkspaceName'), ')')]",
                "galleryName": "Security"
            },
            {
                "name": "[concat('SecurityCenterFree', '(', parameters('LogAnalyticsWorkspaceName'), ')')]",
                "galleryName": "SecurityCenterFree"
            },
            {
                "name": "[concat('ChangeTracking', '(', parameters('logAnalyticsWorkspaceName'), ')')]",
                "galleryName": "ChangeTracking"
            },
            {
                "name": "[concat('KeyVaultAnalytics', '(', parameters('LogAnalyticsWorkspaceName'), ')')]",
                "galleryName": "KeyVaultAnalytics"
            },
            {
                "name": "[concat('AzureSQLAnalytics', '(', parameters('LogAnalyticsWorkspaceName'), ')')]",
                "galleryName": "AzureSQLAnalytics"
            },
            {
                "name": "[concat('ServiceMap', '(', parameters('LogAnalyticsWorkspaceName'), ')')]",
                "galleryName": "ServiceMap"
            }
