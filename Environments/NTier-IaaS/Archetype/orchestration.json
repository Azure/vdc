                "OverrideParameters": {
                    "virtualMachineScaleSetsName": {
                        "value": "${Parameters.ModuleConfigurationParameters.WebApp.Name}"
                    },
                    "virtualMachineScaleSetsSku": {
                        "value": "${Parameters.ModuleConfigurationParameters.WebApp.VMSKU}"
