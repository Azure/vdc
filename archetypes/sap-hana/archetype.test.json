                        "version": "1.0"
                    },
                    "dependencies": [
                        "la"
                    ]
                },
                {
                    "module": "iscsi",
                    "source": {
                        "version": "1.0"
                    },
                    "dependencies": [
                        "workload-net"
                    ]
                },
                {
                    "module": "nfs",
                    "source": {
                        "version": "1.0"
                    },
                    "dependencies": [
                        "workload-net"
                    ]
                },
                {
                    "module": "hana",
                    "source": {
                        "version": "1.0"
                    },
                    "dependencies": [
                        "workload-net"
                    ]
                },
                {
                    "module": "ascs",
                    "source": {
                        "version": "1.0"
                    },
                    "dependencies": [
                        "workload-net"
                    ]
                },
                {
                    "module": "netweaver",
                    "source": {
                        "version": "1.0"
                    },
                    "dependencies": [
                        "workload-net"
                    ]
                }
            ]
        }
